<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.zk.arouter_demo">

    <application
        android:name=".MApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".Test1Activity">
        </activity>
        <activity android:name=".Test2Activity">
        </activity>
        <activity android:name=".SchameFilterActivity">

            <!-- Schame -->
            <intent-filter>
                <action android:name="android.intent.action.VIEW"/>

                <category android:name="android.intent.category.DEFAULT"/>
                <category android:name="android.intent.category.BROWSABLE"/>

                <data
                    android:host="m.zk.com"
                    android:scheme="arouter"/>
            </intent-filter>
        </activity>

        <activity android:name=".WebViewActivity"></activity>
        <activity android:name=".UrlActivity"></activity>
        <activity android:name=".InterCeptorActivity"></activity>
        <activity android:name=".ServiceActivity"></activity>
    </application>

</manifest>